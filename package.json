{
  "name": "rhymetime-games",
  "version": "1.0.0",
  "description": "AI-powered word puzzle games that make you a better writer. Play daily rhyming challenges, compete on leaderboards, and improve your vocabulary through addictive gameplay.",
  "keywords": [
    "word games",
    "rhyme puzzles",
    "vocabulary",
    "writing skills",
    "puzzle games",
    "word matching",
    "rhyming",
    "educational games",
    "brain training",
    "word play"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "cd node-backend && npm run dev",
    "build": "vite build",
    "start": "cd node-backend && npm start",
    "test": "concurrently \"vitest\" \"cd node-backend && npm test\"",
    "lint": "concurrently \"cd rhymetime-games && npm run lint\" \"cd node-backend && npm run lint\"",
    "analyze-assets": "node scripts/analyze-unused-assets.js",
    "optimize": "node scripts/optimize-assets.js",
    "clean": "node scripts/optimize-assets.js"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/d3": "^7.4.3",
    "@types/d3-force": "^3.0.10",
    "@types/firebase": "^2.4.32",
    "@types/node": "^20.14.8",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/stripe": "^8.0.416",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.0",
    "d3": "^7.9.0",
    "d3-force": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^1.0.1",
    "vitest": "^2.0.4",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.24.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@google-cloud/translate": "^8.0.2",
    "@mui/icons-material": "^5.15.15",
    "@mui/material": "^5.15.15",
    "@stripe/stripe-js": "^4.1.0",
    "@types/html2canvas": "^0.5.35",
    "firebase": "^10.12.2",
    "framer-motion": "^11.2.10",
    "html2canvas": "^1.4.1",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.5.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^14.1.2",
    "react-router-dom": "^6.23.1",
    "recharts": "^2.15.4",
    "web-vitals": "^4.2.0"
  }
}
